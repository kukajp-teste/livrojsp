<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>livro-jsp</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <servlet-name>Controller</servlet-name>
    <servlet-class>br.com.lapaz.livrojsp.controller.Controller</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Controller</servlet-name>
    <url-pattern>/principal</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <servlet>
    <description></description>
    <display-name>MeuServlet</display-name>
    <servlet-name>MeuServlet</servlet-name>
    <servlet-class>br.com.lapaz.livrojsp.controller.MeuServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MeuServlet</servlet-name>
    <url-pattern>/meuservlet</url-pattern>
  </servlet-mapping>
</web-app>